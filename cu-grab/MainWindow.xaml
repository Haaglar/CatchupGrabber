<Window x:Class="cu_grab.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="cu-grab" Height="500" Width="500"
        ResizeMode="NoResize">
    <Grid>
        <TextBlock x:Name="errorLabel" HorizontalAlignment="Left" Margin="295,418,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.628,0.369" Height="54" TextWrapping="Wrap" Width="199" Text="Double click on a show, the double click on an episode to get."/>
        <ListBox x:Name="objectList" HorizontalAlignment="Left" Height="217" Margin="70,34,0,0" VerticalAlignment="Top" Width="382" MouseDoubleClick="oL_ItemPressed"/>
        <Button x:Name="toShows" Content="Shows" HorizontalAlignment="Left" Margin="362,268,0,0" VerticalAlignment="Top" Width="75" Click="Shows_Pressed" Height="28"/>
        <Button x:Name="ButtonTenplay" Content="Tenplay" HorizontalAlignment="Left" Margin="47,395,0,0" VerticalAlignment="Top" Width="75" Click="ButtonTenplay_Click"/>
        <Button x:Name="ButtonRTVEC" Content="RTVE Clan" HorizontalAlignment="Left" Margin="159,395,0,0" VerticalAlignment="Top" Width="75" Click="ButtonRTVEC_Click"/>
        <TextBox x:Name="TextBoxProxy" HorizontalAlignment="Left" Height="23" Margin="295,326,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="156" IsEnabled="False"/>
        <Label Content="Gylpe Proxy:" HorizontalAlignment="Left" Margin="215,324,0,0" VerticalAlignment="Top" Height="25" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
        <ProgressBar x:Name="ProgressBarDL" HorizontalAlignment="Left" Height="28" Margin="82,268,0,0" VerticalAlignment="Top" Width="178"/>

    </Grid>
</Window>
